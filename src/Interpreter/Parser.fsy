%{
open FSlispy.Runtime
%}

%token <int> INT
%token <string> SYMBOL

%token EOF LPAREN RPAREN

%start lispy
%type <FSlispy.Runtime.Expression> lispy
%%

number: INT	{ Number($1) }
symbol: SYMBOL { Symbol( { Name=$1 } ) }
sexpr: LPAREN exprList RPAREN { List($2) }

expr: number               	{ $1 }
	| symbol	    		{ $1 }
	| sexpr					{ $1 }

exprList: expr exprList		{ $1 :: $2}
		| 					{ [] }

lispy: exprList EOF        	{ List($1) }